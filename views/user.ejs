<%- include('./partials/header')%>
<h3><a class="ui right floated primary button" href="/items/add">Add Item</a></h3><br><br>
<table class="ui celled compact table">
  <thead>
    <tr>
      <th>No</th>
      <th>Name</th>
      <th>Role</th>
      <th>status</th>
      <th>Email</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <%users.forEach((data,no) => {%>
      <td><%=no+1%></td>
      <td><%=data.name%></td>
      <td><%=data.getRole()%></td>
      <td><%=data.getStatus()%></td>
      <td><%=data.email%></td>
      <td><a href="/users/change/<%=data.id%>">Change to Admin</a> || <a href="/users/changeU/<%=data.id%>">Change to User</a></td>
    </tr>
    <%})%>
  </tbody>
</table>
<%- include('./partials/footer')%>
